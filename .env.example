# LLM Configuration
OPENROUTER_API_KEY=your_openrouter_key_here
LLM_MODEL=google/gemini-3-flash-preview
SEARCH_MODEL=google/gemini-3-flash-preview:online
EXTRACTION_MODEL=google/gemini-3-flash-preview

# Alternative Models
# LLM_MODEL=anthropic/claude-haiku-4.5
# SEARCH_MODEL=anthropic/claude-haiku-4.5:online
# EXTRACTION_MODEL=anthropic/claude-haiku-4.5

# Calendar Integration
ICAL_URL= #https://calendar.google.com/calendar/ical/.../basic.ics must be able to access with no authentication
TZ=America/New_York  # Standard Unix timezone (leave empty to use system local)

# Authentication (Chainlit, Vault-Web, WebDAV)
AUTH_USERNAME=admin
AUTH_PASSWORD=your_secure_password_here

# Chainlit Auth Secret
CHAINLIT_AUTH_SECRET=long_random_string_here_for_chainlit_auth